{
  "name": "@fraserelliott/fe-components",
  "description": "Reusable React components",
  "version": "0.2.1",
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./stylesheet": "./dist/index.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup src/index.js --format esm --watch",
    "prepublishOnly": "npm run build",
    "demo": "cd demo && npm run dev",
    "pack:demo": "npm run build && npm pack --silent && ls -t *.tgz | head -n 1 | xargs -I {} cp -f \"{}\" fe-components.tgz",
    "refresh-demo": "npm run pack:demo && (cd demo && rm -f package-lock.json && rm -rf node_modules/@fraserelliott/fe-components && npm install)"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@fraserelliott/fe-utilities": "^0.3.0",
    "uuid": "^13.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": [
    "*.css"
  ]
}
